{
  "version": 2,
  "builds": [
    {
      "src": "car_expo/frontend/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "car_expo/backend/src/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "car_scraper/api.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/car_expo/backend/src/index.js"
    },
    {
      "src": "/scraper/(.*)",
      "dest": "/car_scraper/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/car_expo/frontend/$1"
    }
  ],
  "functions": {
    "car_expo/backend/src/index.js": {
      "maxDuration": 30
    },
    "car_scraper/api.py": {
      "maxDuration": 60
    }
  }
}
